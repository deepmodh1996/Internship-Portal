<div id="menu">
    <ul>
      <li class="last"><a href="#">All Companies</a></li>
    </ul>
  </div>
<div id="welcome">

<p id="notice"><%= notice %></p>

<h1>Company Informations</h1>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>Url</th>
      <th>Contact person</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_informations.each do |company_information| %>
      <tr>
        <td><%= company_information.name %></td>
        <td><%= company_information.city %></td>
        <td><%= company_information.url %></td>
        <td><%= company_information.contact_person %></td>
        <td><%= company_information.nature %></td>
        <!-- <td><%#= link_to 'Show', company_information %></td> -->
        <td><%= link_to 'Show', edit_company_information_path(company_information) %></td>
        <td><%= link_to 'Destroy', company_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company Information', new_company_information_path %>
<%= link_to 'Login Page', root_path, class: "btn btn-primary btn-sm float-R" %>
</div>
